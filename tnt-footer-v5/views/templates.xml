<odoo>
    <data>
        <template id="tnt_custom_footer" inherit_id="website.layout">
			<xpath expr="//footer/div[@class='container hidden-print']" position="replace">
                <div class="footer-section">
                    <div class="jewellry-footer">
                        <!-- footer-newsletter - start -->
                        <div class="footer-newsletter bg-black">
                            <div class="container">
                                <div class="row">
                                    <div class="col-md-10 col-md-offset-1">
                                        <div class="row">

                                            <!-- newsletter-content - start -->
                                            <div class="col-lg-6 col-md-6 col-sm-12">
                                                <div class="newsletter-content">
                                                    <h2 class="color-white">Newsletter Signup</h2>
                                                    <p class="color-white">Sign up our Newsletter for Exclusive Discount codes</p>
                                                </div>
                                            </div>
                                            <!-- newsletter-content - end -->

                                            <!-- newsletter-content - start -->
                                            <div class="col-lg-6 col-md-6 col-sm-12">
                                                <div class="newsletter-form">
                                                    <form action="#!">
                                                        <div class="form-item m-0">
                                                            <input type="email" id="footer-newsletter" placeholder="Your Email Here" />
                                                            <button type="submit" class="submit-btn bg-gplus-red">sign up</button>
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                            <!-- newsletter-content - end -->

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- footer-newsletter - end -->
                        <!-- footer-middle - start -->
                        <div class="footer-middle bg-black sec-ptb-60">
                            <div class="container">
                                <div class="row">
                                    <div class="col-lg-3 col-md-6 col-sm-12">
                                        <div class="footer-contact">
                                            <h2 class="footer-title">contact info</h2>
                                            <div class="contact-info-list ul-li-block mb-30">
                                                <ul class="clearfix">
                                                    <li>
                                                        <span class="icon">
                                                            <i class="flaticon-maps-and-flags"></i>
                                                        </span>
                                                        8 Liang Seah Street , Liang Seah Court #01-02 America 189029
                                                    </li>
                                                    <li>
                                                        <span class="icon">
                                                            <i class="flaticon-phone-call"></i>
                                                        </span>
                                                        <strong>Phone:</strong> (+888) 234 789 (+777) 222 888
                                                    </li>
                                                    <li>
                                                        <span class="icon">
                                                            <i class="flaticon-e-mail-envelope"></i>
                                                        </span>
                                                        <strong>Email:</strong> contact@company.com
                                                    </li>
                                                    <li>
                                                        <span class="icon">
                                                            <i class="flaticon-clock-circular-outline"></i>
                                                        </span>
                                                        <strong>Hours:</strong> 7 Days a week from 10:00 am
                                                    </li>
                                                </ul>
                                            </div>

                                            <div class="social-links ul-li">
                                                <ul class="clearfix">
                                                    <li><a href="#!"><i class="fa fa-twitter"></i></a></li>
                                                    <li><a href="#!"><i class="fa fa-facebook-f"></i></a></li>
                                                    <li><a href="#!"><i class="fa fa-pinterest-p"></i></a></li>
                                                    <li><a href="#!"><i class="fa fa-youtube"></i></a></li>
                                                    <li><a href="#!"><i class="fa fa-instagram"></i></a></li>
                                                </ul>
                                            </div>

                                        </div>
                                    </div>

                                    <div class="col-lg-2 col-md-6 col-sm-12">
                                        <div class="useful-links ul-li-block">
                                            <h2 class="footer-title">my account</h2>
                                            <ul class="clearfix">
                                                <li><a href="#!">Sign In</a></li>
                                                <li><a href="#!">View Cart</a></li>
                                                <li><a href="#!">My Wishlist</a></li>
                                                <li><a href="#!">Track My Order</a></li>
                                                <li><a href="#!">Help</a></li>
                                            </ul>
                                        </div>
                                    </div>

                                    <div class="col-lg-2 col-md-6 col-sm-12">
                                        <div class="useful-links ul-li-block">
                                            <h2 class="footer-title">why us</h2>
                                            <ul class="clearfix">
                                                <li><a href="#!">About Us</a></li>
                                                <li><a href="#!">Our Services</a></li>
                                                <li><a href="#!">Best Packages</a></li>
                                                <li><a href="#!">Testimonial</a></li>
                                                <li><a href="#!">Group Sales</a></li>
                                            </ul>
                                        </div>
                                    </div>

                                    <div class="col-lg-2 col-md-6 col-sm-12">
                                        <div class="useful-links ul-li-block">
                                            <h2 class="footer-title">information</h2>
                                            <ul class="clearfix">
                                                <li><a href="#!">Our Blog</a></li>
                                                <li><a href="#!">About Our Shop</a></li>
                                                <li><a href="#!">Secure Shopping</a></li>
                                                <li><a href="#!">Delivery infomation</a></li>
                                                <li><a href="#!">Privacy Policy</a></li>
                                            </ul>
                                        </div>
                                    </div>

                                    <div class="col-lg-3 col-md-6 col-sm-12">
                                        <div class="footer-instagram ul-li">
                                            <h2 class="footer-title">WE ARE ASSOCIATED WITH</h2>
                                            <ul class="clearfix">
                                                <li><a href="#!"><img src="/tnt-footer-v5/static/src/img/logo-nma.png" alt="image_not_found" /></a></li>
                                                <li><a href="#!"><img src="/tnt-footer-v5/static/src/img/asso4.png" alt="image_not_found" /></a></li>
                                                <li><a href="#!"><img src="/tnt-footer-v5/static/src/img/ntb-logo.png" alt="image_not_found" /></a></li>
                                                <li><a href="#!"><img src="/tnt-footer-v5/static/src/img/pata.png" alt="image_not_found" /></a></li>
                                                <li><a href="#!"><img src="/tnt-footer-v5/static/src/img/keep.png" alt="image_not_found" /></a></li>

                                            </ul>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <!-- footer-middle - end -->
                        <!-- footer-bottom - start -->
                        <div class="footer-bottom text-center">
                            <div class="container">

                                <p class="copyright-text m-0">
                                    Copyright © 2018 Tours and Travel. All rights reserved
                                </p>
                                <div id="google_translate_element"></div>
                            </div>
                        </div>
                        <!-- footer-bottom - end -->
                    </div>
                </div>
			</xpath>
            <xpath expr="//footer/div[@class='container mt16 mb8']" position="replace"/>
        </template>
    </data>
</odoo>